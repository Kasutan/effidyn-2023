{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom" : false,
			"customDuotone": false,
			"customGradient": false,
			"defaultPalette": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"palette": [
				{
					"slug": "bleu-1fc",
					"color": "#EBF1FC",
					"name": "Bleu 1FC"
				},
				{
					"slug": "bleu-f4",
					"color": "#CAD6F4",
					"name": "Bleu F4"
				},
				{
					"slug": "bleu-8fc",
					"color": "#C7D8FC",
					"name": "Bleu 8FC"
				},	
				{
					"slug": "bleu-9f",
					"color": "#0B289F",
					"name": "Bleu 9F"
				},	
				{
					"slug": "bleu-9a",
					"color": "#04329A",
					"name": "Bleu 9A"
				},
				{
					"slug": "violet-d2",
					"color": "#8E00D2",
					"name": "Violet D2"
				},
				{
					"slug": "violet-cb",
					"color": "#801ACB",
					"name": "Violet CB"
				},
				{
					"slug": "violet-ba",
					"color": "#491BBA",
					"name": "Violet BA"
				},
				{
					"slug": "rose-ff",
					"color": "#FF00FF",
					"name": "Rose FF"
				},
				{
					"slug": "rose-fc",
					"color": "#FA01FC",
					"name": "Rose FC"
				},
				{
					"slug": "rose-ec",
					"color": "#D000EC",
					"name": "Rose EC"
				},
				{
					"slug": "rose-e4",
					"color": "#BC0DE4",
					"name": "Rose E4"
				},
				{
					"slug": "gris-fa",
					"color": "#FAFAFA",
					"name": "Gris FA"
				},
				{
					"slug": "noir",
					"color": "#2B2A2D",
					"name": "Noir"
				},
				{
					"slug": "blanc",
					"color": "#fff",
					"name": "Blanc"
				}
			]
		},
		"custom": {
			"typography": {
				"font-size": {
					"small": "clamp(1.2rem,1.8vw,1.8rem)",
					"medium":  "clamp(1.5rem,2vw,2rem)",
					"large":  "clamp(1.8rem,2.6vw,2.6rem)",
					"huge":  "clamp(2.2rem,3.2vw,3.2rem)",
					"gigantic": "clamp(2.6rem,4vw,4.6rem)",
					"colossal": "clamp(2.6rem,5vw,7rem)"
				},
				"line-height": {
					"tiny": "1.15",
					"small": "1.2",
					"medium": "1.3",
					"normal": "1.4"
				}
			}
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			],
			"fontSizes": [
				{
					"size": "var(--wp--custom--typography--font-size--small)",
					"slug": "small"
				},
				{
					"size": "var(--wp--custom--typography--font-size--medium)",
					"slug": "medium"
				},
				{
					"size": "var(--wp--custom--typography--font-size--large)",
					"slug": "large"
				},
				{
					"size": "var(--wp--custom--typography--font-size--huge)",
					"slug": "x-large"
				}
			]
		},
		"layout": {
			"smallSize": "840px",			
			"contentSize": "1200px",
			"wideSize": "1440px",			
			"fullSize": "1920px"

		}
	},
	"styles": {
		"color": {
			"background": "#ffffff",
			"text": "var(--wp--preset--color--bleu-9f)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "1px"
				},
				"color": {
					"background": "transparent",
					"text": "var(--wp--preset--color--bleu-9a)"
				}
			}
			
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--small)"
		}
	},
	"customTemplates": [],
	"templateParts": []
}
